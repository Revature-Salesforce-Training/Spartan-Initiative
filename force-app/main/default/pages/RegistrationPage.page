<apex:page Standardcontroller="Registration_Entry__c" standardStylesheets="false" showHeader="false" sidebar="false" >
<head>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous"/>
</head>
<apex:pageBlock title="COME JOIN US AT THE NEXT TRADE SHOW">
 <apex:form >
  <div class="card bg-dark text-white">
    <img class="card-img" src="{!$Resource.Trade_Show}"/>
     <apex:detail > 
            <apex:pageBlockSection columns="1">
              <apex:repeat value="{!$Objecttype.Registration_Entry__c.FieldSets.My_Field_Set}" var="field">
                <apex:inputField value="{!Registration_Entry__c[field]}">
                </apex:inputField>
              </apex:repeat>
            <apex:commandButton value="Save" action="{!save}"></apex:commandButton>
          </apex:pageBlockSection>
      </apex:detail>
  </div>
</apex:form>
</apex:pageBlock>
</apex:page>